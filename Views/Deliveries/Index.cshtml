@model IEnumerable<DeliveryDetails>

@{
    ViewData["Title"] = "Orders";
}

<h1>Delivery Details</h1>

<table class="table sorting" id="deliveries">
    <thead>
        <tr>
            <th>
                First Name
            </th>
            <th>
                Last Name
            </th>
            <th>
                Customer Address
            </th>
            <th>
                E-mail
            </th>
            <th>
                Phone Number
            </th>
            <th>
                Delivery Agent Name
            </th>
            <th>
                Delivery Date
            </th>
    <tbody>
        @foreach (var item in Model)
        {
            <tr id="id_@item.Id">
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
                <td>
                    @item.Id
                </td>
            </tr>
        }
    </tbody>
</table>
